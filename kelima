    MainFrame.Visible = false
    MinimizedFrame.Visible = true
end)

RestoreBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = true
    MinimizedFrame.Visible = false
end)

CloseBtn.MouseButton1Click:Connect(function()
    if Settings.Enabled then
        Notify("Deactivate first before closing!")
    else
        ScreenGui:Destroy()
    end
end)

EnableBtn.MouseButton1Click:Connect(function()
    Settings.Enabled = not Settings.Enabled
    if Settings.Enabled then
        EnableBtn.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
        EnableBtn.Text = "‚úÖ EXTREME MODE ACTIVE"
        EnableStroke.Color = Color3.fromRGB(150, 255, 150)
        StatusLabel.Text = "üî• ACTIVE - EXPLODING GAINS!"
        StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        Notify(string.format("ACTIVATED! Each train: +%s", FormatNumber(180 * Settings.GainMultiplier)))
    else
        EnableBtn.BackgroundColor3 = Color3.fromRGB(150, 50, 200)
        EnableBtn.Text = "üöÄ ACTIVATE EXTREME MODE"
        EnableStroke.Color = Color3.fromRGB(200, 100, 255)
        StatusLabel.Text = "‚≠ï INACTIVE"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 150, 150)
        Notify("DEACTIVATED")
    end
end)

AutoBtn.MouseButton1Click:Connect(function()
    Settings.AutoTrain = not Settings.AutoTrain
    if Settings.AutoTrain then
        AutoBtn.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
        AutoBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
        AutoBtn.Text = "ü§ñ AUTO TRAIN: ON"
        Notify("Auto Train: ON")
    else
        AutoBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
        AutoBtn.TextColor3 = Color3.fromRGB(200, 200, 220)
        AutoBtn.Text = "ü§ñ AUTO TRAIN: OFF"
        Notify("Auto Train: OFF")
    end
end)

-- EXTREME AGGRESSIVE HOOKING
local oldNamecall
oldNamecall = hookmetamethod(game, "__namecall", newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    
    if not Settings.Enabled then
        return oldNamecall(self, ...)
    end
    
    -- Hook InvokeServer - EXTREME MULTIPLY
    if method == "InvokeServer" then
        local results = {oldNamecall(self, ...)}
        
        for i = 1, #results do
            if typeof(results[i]) == "number" and results[i] > 10 then
                local original = results[i]
                results[i] = results[i] * Settings.GainMultiplier
                local gained = results[i] - original
                Stats.TotalGains = Stats.TotalGains + gained
                Stats.CombatPowerGained = Stats.CombatPowerGained + (gained * 0.1) -- Estimate
            end
        end
        
        return unpack(results)
    end
    
    -- Hook FireServer - EXTREME SPEED
    if method == "FireServer" then
        if self.Name == "TrainSpeedHasChanged" and args[1] then
            args[1] = 999999 -- Maximum speed
            Stats.TrainingSessions = Stats.TrainingSessions + 1
            task.spawn(UpdateUI)
        end
        return oldNamecall(self, unpack(args))
    end
    
    return oldNamecall(self, ...)
end))

-- Hook ALL BindableFunctions - EXTREME BOOST
task.spawn(function()
    task.wait(2)
    for _, service in pairs({ReplicatedStorage, workspace}) do
        for _, descendant in pairs(service:GetDescendants()) do
            if descendant:IsA("BindableFunction") then
                pcall(function()
                    local oldInvoke = descendant.Invoke
                    descendant.Invoke = newcclosure(function(self, ...)
                        local result = oldInvoke(self, ...)
                        
                        if Settings.Enabled and typeof(result) == "number" and result > 10 then
                            return result * Settings.GainMultiplier
                        end
                        
                        return result
                    end)
                end)
            end
        end
    end
end)

-- Hook TrainSystem - DIRECT EXTREME BOOST
task.spawn(function()
    local TrainSystem = ReplicatedStorage:WaitForChild("TrainSystem", 10)
    if TrainSystem then
        -- Hook PlayerTrainValueHasChanged
        local trainChanged = TrainSystem.Remote:FindFirstChild("PlayerTrainValueHasChanged")
        if trainChanged then
            trainChanged.OnClientEvent:Connect(function(plr, trainType, newVal, oldVal)
                if Settings.Enabled and newVal and oldVal and plr == player then
                    local original = newVal - oldVal
                    local boosted = original * (Settings.GainMultiplier - 1)
                    Stats.TotalGains = Stats.TotalGains + boosted
                    Stats.CombatPowerGained = Stats.CombatPowerGained + (boosted * 0.1)
                    task.spawn(UpdateUI)
                    
                    print(string.format("üíé [EXTREME BOOST] Type %d: +%d ‚Üí +%s", 
                        trainType, original, FormatNumber(original * Settings.GainMultiplier)))
                end
            end)
        end
        
        -- Hook GetTrainValue
        local getTrainValue = TrainSystem.Bindable:FindFirstChild("GetTrainValue")
        if getTrainValue then
            local old = getTrainValue.Invoke
            getTrainValue.Invoke = newcclosure(function(self, ...)
                local val = old(self, ...)
                if Settings.Enabled and typeof(val) == "number" then
                    return val * Settings.GainMultiplier
                end
                return val
            end)
        end
        
        -- Hook GetCombatPower - BOOST COMBAT POWER
        local getCombatPower = TrainSystem.Bindable:FindFirstChild("GetCombatPower")
        if getCombatPower then
            local old = getCombatPower.Invoke
            getCombatPower.Invoke = newcclosure(function(self, ...)
                local val = old(self, ...)
                if Settings.Enabled and Settings.BoostCombatPower and typeof(val) == "number" then
                    return val * math.min(Settings.GainMultiplier / 100, 100) -- Cap untuk safety
                end
                return val
            end)
        end
        
        -- Hook PlayerCombatPowerHasChanged
        local combatChanged = TrainSystem.Remote:FindFirstChild("PlayerCombatPowerHasChanged")
        if combatChanged then
            combatChanged.OnClientEvent:Connect(function(plr, newPower)
                if Settings.Enabled and plr == player then
                    print(string.format("‚öîÔ∏è [COMBAT POWER] New Power: %s", FormatNumber(newPower)))
                end
            end)
        end
    end
end)

-- Hook Statistics System
task.spawn(function()
    local Statistics = ReplicatedStorage:WaitForChild("Statistics", 10)
    if Statistics then
        local statsChanged = Statistics.Remote:FindFirstChild("StatisticsDataHasChanged")
        if statsChanged then
            statsChanged.OnClientEvent:Connect(function(pointsGained, trainType)
                if Settings.Enabled and pointsGained then
                    local boosted = pointsGained * Settings.GainMultiplier
                    print(string.format("üìä [STATS] Type %d: +%d ‚Üí +%s", 
                        trainType or 0, pointsGained, FormatNumber(boosted)))
                end
            end)
        end
    end
end)

-- EXTREME Auto Train Loop
task.spawn(function()
    while task.wait(0.1) do
        if Settings.AutoTrain and Settings.Enabled then
            pcall(function()
                local TrainEquip = ReplicatedStorage:FindFirstChild("TrainEquipment")
                if TrainEquip and TrainEquip:FindFirstChild("Remote") then
                    -- Spam both training types for maximum gain
                    local stationary = TrainEquip.Remote:FindFirstChild("ApplyStationaryTrain")
                    if stationary then
                        stationary:InvokeServer()
                    end
                    
                    task.wait(0.05)
                    
                    local mobile = TrainEquip.Remote:FindFirstChild("ApplyMobileTrain")
                    if mobile then
                        mobile:InvokeServer()
                    end
                    
                    -- Fire training speed
                    local trainSpeed = ReplicatedStorage:FindFirstChild("TrainSystem")
                    if trainSpeed and trainSpeed:FindFirstChild("Remote") then
                        local speedRemote = trainSpeed.Remote:FindFirstChild("TrainSpeedHasChanged")
                        if speedRemote then
                            speedRemote:FireServer(999999)
                        end
                    end
                end
            end)
        end
    end
end)

-- Combat Power Booster Loop
task.spawn(function()
    while task.wait(5) do
        if Settings.Enabled and Settings.BoostCombatPower then
            pcall(function()
                local TrainSystem = ReplicatedStorage:FindFirstChild("TrainSystem")
                if TrainSystem and TrainSystem:FindFirstChild("Bindable") then
                    local combatPowerChanged = TrainSystem.Bindable:FindFirstChild("PlayerCombatPowerHasChanged")
                    if combatPowerChanged then
                        local getCombatPower = TrainSystem.Bindable:FindFirstChild("GetCombatPower")
                        if getCombatPower then
                            local currentPower = getCombatPower:Invoke(player)
                            if currentPower then
                                local boostedPower = math.floor(currentPower * 1.5) -- 50% boost every 5 seconds
                                combatPowerChanged:Fire(player, boostedPower)
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- Initialize
UpdateUI()
Notify("EXTREME BOOSTER Loaded!")


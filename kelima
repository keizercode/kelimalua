-- Combat Power Display
local CPFrame = Instance.new("Frame")
CPFrame.Parent = ContentFrame
CPFrame.BackgroundColor3 = Color3.fromRGB(40, 30, 50)
CPFrame.Position = UDim2.new(0.08, 0, 0.82, 0)
CPFrame.Size = UDim2.new(0.84, 0, 0, 50)

local CPCorner = Instance.new("UICorner")
CPCorner.CornerRadius = UDim.new(0, 10)
CPCorner.Parent = CPFrame

local CombatPowerLabel = Instance.new("TextLabel")
CombatPowerLabel.Parent = CPFrame
CombatPowerLabel.BackgroundTransparency = 1
CombatPowerLabel.Size = UDim2.new(1, 0, 1, 0)
CombatPowerLabel.Font = Enum.Font.GothamBold
CombatPowerLabel.Text = "ðŸ’ª Combat Power: Loading..."
CombatPowerLabel.TextColor3 = Color3.fromRGB(255, 200, 50)
CombatPowerLabel.TextSize = 15

-- Update Combat Power with animation
spawn(function()
    while wait(0.5) do
        pcall(function()
            local GetCombatPower = getRemote({"TrainSystem", "Bindable", "GetCombatPower"})
            if GetCombatPower then
                local cp = GetCombatPower:Invoke(player)
                if cp then
                    local formatted = tostring(cp):reverse():gsub("(%d%d%d)", "%1,"):reverse():gsub("^,", "")
                    CombatPowerLabel.Text = "ðŸ’ª Combat Power: " .. formatted
                    
                    -- Color based on power
                    if cp >= 1000000000000 then -- Trillion
                        CombatPowerLabel.TextColor3 = Color3.fromRGB(255, 50, 255)
                    elseif cp >= 1000000000 then -- Billion
                        CombatPowerLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                    elseif cp >= 1000000 then -- Million
                        CombatPowerLabel.TextColor3 = Color3.fromRGB(255, 200, 50)
                    end
                end
            end
        end)
    end
end)

-- Toggle Training Button
SpamButton.MouseButton1Click:Connect(function()
    autoTrainEnabled = not autoTrainEnabled
    
    if autoTrainEnabled then
        SpamButton.Text = "ðŸ›‘ STOP ULTRA SPAM"
        SpamButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        StatusLabel.Text = "Status: ULTRA SPAM ACTIVE ðŸ”¥"
        StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        ultraSpamTrain()
    else
        SpamButton.Text = "ðŸš€ START ULTRA SPAM"
        SpamButton.BackgroundColor3 = Color3.fromRGB(255, 50, 150)
        StatusLabel.Text = "Status: Inactive â­•"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

-- Minimize
MinimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    if isMinimized then
        MainFrame:TweenSize(UDim2.new(0, 400, 0, 55), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.3, true)
        MinimizeButton.Text = "â–¡"
        ContentFrame.Visible = false
    else
        MainFrame:TweenSize(UDim2.new(0, 400, 0, 550), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.3, true)
        MinimizeButton.Text = "â€”"
        wait(0.1)
        ContentFrame.Visible = true
    end
end)

-- Close
CloseButton.MouseButton1Click:Connect(function()
    autoTrainEnabled = false
    MainFrame:TweenSize(UDim2.new(0, 0, 0, 0), Enum.EasingDirection.In, Enum.EasingStyle.Back, 0.3, true)
    wait(0.3)
    ScreenGui:Destroy()
end)


    SpeedLabel.Text = "âš¡ Training Speed: " .. trainingSpeed .. (trainingSpeed == 10 and " (MAX)" or "")
    SpeedLabel.TextColor3 = trainingSpeed == 10 and Color3.fromRGB(255, 255, 100) or Color3.fromRGB(255, 255, 255)
